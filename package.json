    "express": "4.17.1",
    "express-graphql": "0.9.0",
    "graphql": "14.5.8",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.8.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "eslint": "6.0.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "5.0.0",
